  <!-- MESSAGES -->

  <string name="msg_feedback">A great app requires a lot of time and lives from the feedback of its users.</string>
  <string name="msg_feedback_contact">Do you have any wishes, comments or questions? Contact me!\nOtherwise I would be very happy about a nice review in the Play Store.</string>
  <string name="msg_recommend">Check out this cool metronome app: \n%1$s</string>
  <string name="msg_vibration_unavailable">Vibration not available on this device. Sound will be used.</string>
  <string name="msg_service_running">Metronome service is running</string>
  <string name="msg_service_running_return">Tap to return to Tack</string>
  <string name="msg_service_description">Service for metronome configuration and playback</string>
  <string name="msg_connection_lost">Service connection lost, app restart required</string>
  <string name="msg_reset">Reset all?</string>
  <string name="msg_reset_description">Do you really want to reset the entire app including song library and settings? Create a backup of your song library first if needed. Reset cannot be undone.</string>
  <string name="msg_reset_timer">Reset timer?</string>
  <string name="msg_reset_timer_description">You can change the default behavior of whether to reset the timer at start in settings.</string>
  <string name="msg_reset_elapsed">Reset elapsed time?</string>
  <string name="msg_reset_elapsed_description">Do you really want to reset elapsed time? This cannot be undone.</string>
  <string name="msg_gain">Play with volume boost?</string>
  <string name="msg_gain_description">Be careful not to play through headphones as loud sounds can cause ear damage!</string>
  <string name="msg_split_screen">Window too small</string>
  <string name="msg_split_screen_description">Tack may have problems to position metronome controls in split-screen mode. Please use a larger window if possible.</string>
  <string name="msg_notification_permission">Notification permission required</string>
  <string name="msg_notification_permission_description">Tack needs permission to show notifications to continue playback in the background by displaying a low-priority notification. If you deny it in the next dialog, Android will stop playback when you leave the app.</string>
  <string name="msg_notification_permission_denied">Notification permission is required for background playback</string>
  <string name="msg_report_crash">Please report any crash with the crash log attached so I can try to fix them.</string>
  <string name="msg_copied_to_clipboard">Copied to clipboard successfully</string>
  <string name="msg_invalid_input">Invalid input</string>
  <string name="msg_discard_changes">Discard changes?</string>
  <string name="msg_discard_changes_description">Are you sure you don\'t want to save them?</string>
  <string name="msg_delete_song">Delete song?</string>
  <string name="msg_delete_song_description">Are you sure you want to delete the entire song with all of its parts? This cannot be undone.</string>
  <string name="msg_songs_empty">Empty song library</string>
  <string name="msg_songs_empty_description">Add songs you want to practice</string>
  <string name="msg_unlock">Message from the developer</string>
  <string name="msg_unlock_description">Do you appreciate the ad-free experience with tons of powerful features in Tack? I\'ve spent years of my spare time creating Tack for free, but especially the song library feature took months of hard work.\nIn order to continue improving Tack, I kindly ask for a small contribution. In return, you\'ll get an unlock app for unlimited songs and countless song parts. Thanks for your support!</string>
  <string name="msg_backup_error">Error during backup</string>
  <string name="msg_backup_directory_missing">No storage location selected for backup</string>
  <string name="msg_backup_success">Backup created successfully</string>
  <string name="msg_restore_error">Error while restoring backup</string>
  <string name="msg_restore_file_missing">No file selected for restoring</string>
  <string name="msg_restore_success">Backup restored successfully</string>
  <!-- Name of a song after restoring a backup when a song with the name already exists. E.g. "Song name (1)" -->
  <string name="msg_restore_duplicate_name">%1$s (%2$d)</string>
  <string name="msg_songs_intro">Bookmarks are songs now</string>
  <string name="msg_songs_intro_description">The new song library replaces the previous tempo bookmarks. Instead of bookmarking bpm values, you can now save and arrange entire metronome configurations as parts of a song. Simply adjust the controls and options to your liking and add the current configuration as a new part to a song or update an existing part.</string>
